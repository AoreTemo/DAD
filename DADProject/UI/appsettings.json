{
  "Seriolog" : {
    "Using" : "Seriolog.Sinks.File",
    "WriteTo" : [
      {"Name" : "File",
        "Args": {
          "path" : "logs/log-.txt",
          "rollingInterval" : "Day"
        }
      } 
    ]
  },
  "ConnectionStrings": {
    "DefaultConnection": ""
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
